{
  "name": "tree-sitter-flang",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for FLang",
  "main": "bindings/node",
  "keywords": [
    "tree-sitter",
    "parser",
    "flang"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.22.0"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp rebuild",
    "generate": "tree-sitter generate",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse"
  },
  "tree-sitter": [
    {
      "scope": "source.flang",
      "file-types": [
        "f"
      ],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm"
    }
  ]
}
